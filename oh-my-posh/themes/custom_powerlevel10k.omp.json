{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os_bg": "#FFFFFF",
    "os_fg": "#000000",
    "path_bg": "#06B0FF",
    "path_fg": "#FFFFFF",
    "git_bg": "#FF4A7B",
    "git_fg": "#000000",
    "python_bg": "#06B0FF",
    "python_fg": "#000000",
    "node_bg": "#68a063",
    "node_fg": "#FFFFFF",
    "go_bg": "#00ADD8",
    "go_fg": "#FFFFFF",
    "ruby_bg": "#CC342D",
    "ruby_fg": "#FFFFFF",
    "java_bg": "#ED8B00",
    "java_fg": "#FFFFFF",
    "julia_bg": "#9558B2",
    "julia_fg": "#FFFFFF",
    "php_bg": "#777BB4",
    "php_fg": "#FFFFFF",
    "exec_bg": "#ACB0BE",
    "exec_fg": "#000000",
    "status_bg": "#28a745",
    "status_fg": "#FFFFFF",
    "error_bg": "#dc3545",
    "error_fg": "#FFFFFF"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:os_bg",
          "foreground": "p:os_fg",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": " {{.Icon}} ",
          "type": "os"
        },
        {
          "background": "p:path_bg",
          "foreground": "p:path_fg",
          "properties": {
            "folder_icon": "\uf07c ",
            "home_icon": "~",
            "style": "agnoster_short",
            "max_depth": 2
          },
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:git_bg",
          "foreground": "p:git_fg",
          "style": "powerline",
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "powerline_symbol": "\ue0b0",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#000000",
          "background_templates": ["{{ if gt .Code 0 }}#EE2A69{{ else }}#000000{{ end }}"],
          "foreground_templates": ["{{ if gt .Code 0 }}#FFC900{{ else }}#3ED177{{ end }}"],
          "foreground": "#3ED177",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ if gt .Code 0 }}{{.Code}} \u2718{{ else }}\u2714{{ end }} ",
          "type": "status"
        },
        {
          "background": "p:exec_bg",
          "foreground": "p:exec_fg",
          "properties": {
            "threshold": 0,
            "style": "roundrock"
          },
          "style": "diamond",
          "template": " \uf252 {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "p:python_bg",
          "foreground": "p:python_fg",
          "properties": {
            "display_mode": "environment",
            "fetch_version": true
          },
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " {{ if .Venv }}{{ .Venv }}{{ else }}{{ .Folder }}{{ end }} <p:python_icon>\ue235</> {{ .Full }} ",
          "type": "python"
        },
        {
          "background": "p:node_bg",
          "foreground": "p:node_fg",
          "properties": {
            "display_mode": "context",
            "fetch_version": true
          },
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " Node \ue718 {{ .Full }} ",
          "type": "node"
        },
        {
          "background": "p:go_bg",
          "foreground": "p:go_fg",
          "properties": {
            "display_mode": "context",
            "fetch_version": true
          },
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " Go \ue626 {{ .Full }} ",
          "type": "go"
        },
        {
          "background": "p:ruby_bg",
          "foreground": "p:ruby_fg",
          "properties": {
            "display_mode": "context",
            "fetch_version": true
          },
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " Ruby \ue791 {{ .Full }} ",
          "type": "ruby"
        },
        {
          "background": "p:java_bg",
          "foreground": "p:java_fg",
          "properties": {
            "display_mode": "context",
            "fetch_version": true
          },
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " Java \ue738 {{ .Full }} ",
          "type": "java"
        },
        {
          "background": "p:julia_bg",
          "foreground": "p:julia_fg",
          "properties": {
            "display_mode": "context",
            "fetch_version": true
          },
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " Julia \ue624 {{ .Full }} ",
          "type": "julia"
        },
        {
          "background": "p:php_bg",
          "foreground": "p:php_fg",
          "properties": {
            "display_mode": "context",
            "fetch_version": true
          },
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b4",
          "template": " PHP \ue73d {{ .Full }} ",
          "type": "php"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FF4A7B",
    "template": "‚ùØ "
  },
  "version": 3
}
