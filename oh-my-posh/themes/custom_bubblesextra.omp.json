{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "right",
      "newline": true,
      "segments": [
        {
          "foreground": "#E64747",
          "style": "plain",
          "template": "{{ if not .Error }}{{ else }}\u2718{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "status"
        },
        {
          "foreground_templates": [
            "{{ if lt .Ms 2000 }}#B2FF59{{ end }}",
            "{{ if ge .Ms 2000 }}#E64747{{ end }}"
          ],
          "properties": {
            "style": "austin",
            "threshold": 150
          },
          "style": "diamond",
          "template": "{{ if lt .Ms 2000 }} \udb85\udc0c{{ else }} \udb83\udf86{{ end }} {{ .FormattedMs }}",
          "trailing_diamond": "\ue0b4",
          "type": "executiontime"
        },
        {
          "background": "#29315A",
          "foreground": "#FFDE57",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "python"
        },
        {
          "background": "#29315A",
          "foreground": "#7FD5EA",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue627{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "go"
        },
        {
          "background": "#29315A",
          "foreground": "#42E66C",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "node"
        },
        {
          "background": "#44BAED",
          "foreground": "#29315A",
          "leading_diamond": " \ue0b6",
          "style": "diamond",
          "template": "\ue5ff ",
          "type": "text"
        },
        {
          "background": "#29315A",
          "foreground": "#44BAED",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": " {{ .Path }}",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        },
        {
          "background": "#151B23",
          "foreground": "#D1D7E0",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "leading_diamond": " \ue0b6",
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": "{{ if eq .UpstreamIcon \"\" }}\uf408 {{ else if .UpstreamIcon }}{{ .UpstreamIcon }}{{ else }}\udb80\udea2 {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        },
        {
          "background_templates": [
            "{{ if ge .Percentage 21 }}#9FE3A1{{ end }}",
            "{{ if le .Percentage 20 }}#E64747{{ end }}"
          ],
          "foreground": "#29315A",
          "leading_diamond": " \ue0b6",
          "style": "diamond",
          "template": "{{ if not .Error }}{{ if eq \"Charging\" .State.String }}\uE315 {{ else if eq \"Discharging\" .State.String }}{{ else if eq \"Full\" .State.String }}~ {{ else }}? {{ end }}{{ if le .Percentage 15 }}\udb80\udc7a{{ else if and (ge .Percentage 16) (le .Percentage 30) }}\udb80\udc7b{{ else if and (ge .Percentage 31) (le .Percentage 45) }}\udb80\udc7d{{ else if and (ge .Percentage 46) (le .Percentage 55)}}\udb80\udc7e{{ else if and (ge .Percentage 56) (le .Percentage 70) }}\udb80\udc7f{{ else if and (ge .Percentage 71) (le .Percentage 80) }}\udb80\udc82{{ else if and (ge .Percentage 81) (le .Percentage 95) }}\udb80\udc82{{ else }}\udb80\udc79{{ end }} {{ else }}!{{ end }}",
          "type": "battery"
        },
        {
          "background": "#29315A",
          "foreground_templates": [
            "{{ if ge .Percentage 21 }}#9FE3A1{{ end }}",
            "{{ if le .Percentage 20 }}#E64747{{ end }}"
          ],
          "style": "diamond",
          "template": " {{ .Percentage }}%",
          "trailing_diamond": "\ue0b4",
          "type": "battery"
        },
        {
          "background": "#AEA4BF",
          "foreground": "#29315A",
          "leading_diamond": " \ue0b6",
          "style": "diamond",
          "template": "\udb86\udd9f ",
          "type": "text"
        },
        {
          "background": "#29315A",
          "foreground": "#AEA4BF",
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }}",
          "trailing_diamond": "\ue0b4",
          "type": "time",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#E64747",
          "style": "plain",
          "template": " ⇒",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }} ({{ .Shell }})",
  "final_space": true,
  "version": 3
}
